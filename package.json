{
  "name": "praha-challenge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "eslint": "./node_modules/.bin/eslint --fix ./*/**.js",
    "prettier": "./node_modules/.bin/eslint --writer ./*/**.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "^6.0.0"
  }
}
